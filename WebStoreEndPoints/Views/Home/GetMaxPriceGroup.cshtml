@model IEnumerable<WebStoreEndPoints.Models.StoreItemTemp>
@{ Layout = "_Layout"; ViewBag.Title = "Get Max Price Group";}

<h2> <a asp-action="Index" class="btn btn-sm btn-primary">Back</a></h2>

@if (Model != null)
{
    <h2>Store Item Cost </h2>
    <table class="table table-sm table-striped table-bordered m-2">
        <thead>
            <tr>
                <th>Name</th>
                <th>Cost</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var r in Model)
            {
                <tr>
                    
                    <td>@r.ItemName</td>
                    <td>$@r.Cost</td>
                </tr>
            }
        </tbody>
    </table>
}
