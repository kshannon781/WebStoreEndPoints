@model IEnumerable<WebStoreEndPoints.Models.StoreItem>
@{ Layout = "_Layout"; ViewBag.Title = "Get Max Price Group";}

<h2>Grouped Items <a asp-action="Index" class="btn btn-sm btn-primary">Back</a></h2>
<form method="post">
   
        <button type="submit" class="btn btn-sm btn-primary">Get Groups</button>
    
</form>

@if (Model != null)
{
    <h2>Store Item</h2>
    <table class="table table-sm table-striped table-bordered m-2">
        <thead>
            <tr>
                <th>Name</th>
                <th>Cost</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var r in Model)
            {
                <tr>
                    
                    <td>@r.ItemName</td>
                    <td>@r.Cost</td>
                </tr>
            }
        </tbody>
    </table>
}
